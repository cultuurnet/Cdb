{
    "name": "cultuurnet/cdb",
    "description": "PHP library for hydrating/manipulating CultuurNet's Cdb XML documents",
    "license": "Apache-2.0",
    "autoload": {
        "psr-0": {
            "CultureFeed_Cdb_": ["lib"]
        }
    },
    "autoload-dev": {
      "psr-0": {
        "CultureFeed_Cdb_": ["tests"]
      }
    },
    "require-dev": {
      "phpunit/phpunit": "^9.0",
        "phpstan/phpstan": "^2.1",
        "publiq/php-cs-fixer-config": "^2.1"
    },
    "require": {
      "php": ">=8.1",
      "ext-simplexml": "*",
      "ext-dom": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
