{
    "name": "cultuurnet/cdb",
    "description": "PHP library for hydrating/manipulating CultuurNet's Cdb XML documents",
    "license": "Apache-2.0",
    "autoload": {
        "psr-0": {
            "CultureFeed_Cdb_": ["lib"]
        }
    },
    "autoload-dev": {
      "psr-0": {
        "CultureFeed_Cdb_": ["tests"]
      }
    },
    "require-dev": {
      "phpunit/phpunit": "~4.8",
      "squizlabs/php_codesniffer": "~2.3"
    },
    "require": {
      "ext-simplexml": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
      "platform": {
        "php": "5.6"
      }
    }
}
